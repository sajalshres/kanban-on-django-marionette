{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "type": "pwa-chrome",
      "request": "attach",
      "address": "localhost",
      "port": 9222,
      "name": "Launch Chrome",
      "urlFilter": "http://localhost:8000/*",
      "webRoot": "${workspaceFolder}/frontend/"
    },
    {
      "type": "pwa-msedge",
      "request": "attach",
      "address": "localhost",
      "name": "Launch Edge",
      "urlFilter": "http://localhost:8000/*",
      "webRoot": "${workspaceFolder}/frontend/"
    },
    {
      "name": "Launch firefox",
      "type": "firefox",
      "request": "launch",
      "reAttach": true,
      "url": "http://localhost:8000",
      "webRoot": "${workspaceFolder}/frontend/",
      "firefoxExecutable": "C:/Users/shres/scoop/shims/firefox.exe",
      "sourceMapPathOverrides": {
        "/mnt/c/*": "C:/*"
      }
    }
  ]
}
